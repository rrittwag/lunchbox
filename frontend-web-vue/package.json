{
  "name": "lunchbox-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:unit": "cross-env LANG=de_DE.UTF-8 vitest --watch false",
    "test:unit:report": "cross-env LANG=de_DE.UTF-8 vitest --watch false --reporter=junit --outputFile=tests/unit/report.xml",
    "lint": "yarn lint:check --fix",
    "lint:check": "eslint --no-error-on-unmatched-pattern --ext .vue,.ts,.js src/** tests/**",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "9.1.0",
    "focus-visible": "5.2.0",
    "pinia": "2.0.17",
    "vue": "3.2.37",
    "vue-router": "4.1.3"
  },
  "devDependencies": {
    "@iconify-json/fa-solid": "1.1.2",
    "@pinia/testing": "0.0.13",
    "@rushstack/eslint-patch": "1.1.4",
    "@vitejs/plugin-vue": "3.0.1",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.0",
    "@vue/test-utils": "2.0.2",
    "autoprefixer": "10.4.8",
    "cross-env": "7.0.3",
    "eslint": "8.21.0",
    "eslint-plugin-vue": "9.3.0",
    "jsdom": "20.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "sass": "1.54.4",
    "tailwindcss": "3.1.8",
    "typescript": "4.6.4",
    "unplugin-icons": "0.14.8",
    "vite": "3.0.5",
    "vite-plugin-pwa": "0.12.3",
    "vitest": "0.21.1",
    "vue-router-mock": "0.1.8",
    "vue-tsc": "0.40.0"
  },
  "resolutions": {
    "terser": "5.14.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
