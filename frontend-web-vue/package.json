    "lint": "yarn lint:check --fix",
    "lint:check": "eslint --no-error-on-unmatched-pattern --ext .vue,.ts,.js src/** tests/**",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.3",
    "focus-visible": "^5.2.0",
    "pinia": "^2.0.12",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@iconify-json/fa-solid": "^1.1.1",
    "@pinia/testing": "^0.0.10",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "sass": "^1.49.10",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.5.5",
    "unplugin-icons": "^0.14.1",
    "vite": "^2.9.1",
    "vite-plugin-pwa": "^0.11.13",
    "vitest": "^0.8.1",
    "vue-router-mock": "^0.1.7",
    "vue-tsc": "^0.33.9"
  },
  "overrides": {
    "json5@2.2.0": {
      "minimist": "^1.2.6"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
