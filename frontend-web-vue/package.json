{
  "name": "lunchbox-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:unit": "cross-env LANG=de_DE.UTF-8 vitest --watch false",
    "test:unit:report": "cross-env LANG=de_DE.UTF-8 vitest --watch false --reporter=junit --outputFile=tests/unit/report.xml",
    "lint": "yarn lint:check --fix",
    "lint:check": "eslint --no-error-on-unmatched-pattern --ext .vue,.ts,.js src/** tests/**",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^8.6.0",
    "focus-visible": "^5.2.0",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@iconify-json/fa-solid": "^1.1.1",
    "@pinia/testing": "^0.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "sass": "^1.52.2",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.7.3",
    "unplugin-icons": "^0.14.3",
    "vite": "^2.9.10",
    "vite-plugin-pwa": "^0.12.0",
    "vitest": "^0.14.1",
    "vue-router-mock": "^0.1.7",
    "vue-tsc": "^0.37.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
