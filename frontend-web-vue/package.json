{
  "name": "lunchbox-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:unit": "cross-env LANG=de_DE.UTF-8 vitest --watch false",
    "test:unit:report": "cross-env LANG=de_DE.UTF-8 vitest --watch false --reporter=junit --outputFile=tests/unit/report.xml",
    "lint": "yarn lint:check --fix",
    "lint:check": "eslint --no-error-on-unmatched-pattern --ext .vue,.ts,.js src/** tests/**",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "9.6.0",
    "focus-visible": "5.2.0",
    "pinia": "2.0.27",
    "vue": "3.2.45",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@iconify-json/fa-solid": "1.1.3",
    "@pinia/testing": "0.0.14",
    "@rushstack/eslint-patch": "1.2.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/test-utils": "2.2.6",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "eslint": "8.29.0",
    "eslint-plugin-vue": "9.8.0",
    "jsdom": "20.0.3",
    "prettier": "2.8.1",
    "prettier-plugin-tailwindcss": "0.2.0",
    "sass": "1.56.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "unplugin-icons": "0.14.14",
    "vite": "4.0.0",
    "vite-plugin-pwa": "0.13.3",
    "vitest": "0.25.6",
    "vue-router-mock": "0.1.9",
    "vue-tsc": "1.0.11"
  },
  "resolutions": {
    "terser": "5.16.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
